package edu.mit.blocks.renderable;

import edu.mit.blocks.codeblocks.Block;
import edu.mit.blocks.workspace.Workspace;
import edu.mit.blocks.workspace.WorkspaceEnvironment;

public class BlockExplodeManager {

	private Workspace workspace;
	private WorkspaceEnvironment workspaceEnvironment;
	private Block oriBlock;
	public BlockExplodeManager(Workspace wkspace,Block block){
		this.workspace = wkspace;
		this.workspaceEnvironment = wkspace.getEnv();
		this.oriBlock = block;
	}
	public 
}
